{
  "manifest_version": 3,
  "name": "Choodic",
  "version": "1.0",
  "description": "Translate words and add them to your dictionary",

  "key": "",

  "icons": {
    "16": "images/icon-16.png",
    "32": "images/icon-32.png",
    "48": "images/icon-48.png"
  },
  "content_scripts": [
    {
      "js": ["content.js"],
      "matches": ["<all_urls>"]
    }
  ],
  "action": {},
  "background": {
    "service_worker": "background.js"
  },
  "oauth2": {
    "client_id": "588137183052-og0asjlcg2qt9iraup8iki03a3pikf80.apps.googleusercontent.com",
    "scopes": ["https://www.googleapis.com/auth/userinfo.profile", "https://www.googleapis.com/auth/userinfo.email"]
  },
  "permissions": [ "identity", "identity.email", "storage", "nativeMessaging" ],
  "web_accessible_resources": [
    {
      "resources": [ "images/loader.gif" ],
      "matches": [ "https://*/*" ]
    }
  ]
}
